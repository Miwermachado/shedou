local WindUI = loadstring(game:HttpGet("https://github.com/Footagesus/WindUI/releases/latest/download/main.lua"))()

WindUI:AddTheme({
    Name = "MiweR", -- theme name

    Accent = Color3.fromHex("#18181b"),
    Background = Color3.fromHex("#101010"), -- Accent
    BackgroundTransparency = 0,
    Outline = Color3.fromHex("#FFFFFF"),
    Text = Color3.fromHex("#FFFFFF"),
    Placeholder = Color3.fromHex("#7a7a7a"),
    Button = Color3.fromHex("#52525b"),
    Icon = Color3.fromHex("#ff0000"),
    
    Hover = Color3.fromHex("#FFFFFF"), -- Text
    BackgroundTransparency = 1,
    
    WindowBackground = Color3.fromHex("#000000"), -- Background
    WindowShadow = Color3.fromHex("#2b2b2b"),
    
    DialogBackground = Color3.fromHex("#101010"), -- Background
    DialogBackgroundTransparency = 0, -- BackgroundTransparency
    DialogTitle = Color3.fromHex("#FFFFFF"), -- Text
    DialogContent = Color3.fromHex("#FFFFFF"), -- Text
    DialogIcon = Color3.fromHex("#a1a1aa"), -- Icon
    
    WindowTopbarButtonIcon = Color3.fromHex("a1a1aa"), -- Icon
    WindowTopbarTitle = Color3.fromHex("FFFFFF"), -- Text
    WindowTopbarAuthor = Color3.fromHex("FFFFFF"), -- Text
    WindowTopbarIcon = Color3.fromHex("FFFFFF"), -- Text
    
    TabBackground = Color3.fromHex("#FFFFFF"), -- Text
    TabTitle = Color3.fromHex("#FFFFFF"), -- Text
    TabIcon = Color3.fromHex("#FFFFFF"), -- Icon
    
    ElementBackground = Color3.fromHex("#FFFFFF"), -- Text
    ElementTitle = Color3.fromHex("#FFFFFF"), -- Text
    ElementDesc = Color3.fromHex("#FFFFFF"), -- Text
    ElementIcon = Color3.fromHex("#a1a1aa"), -- Icon
    
    PopupBackground = Color3.fromHex("#101010"), -- Background
    PopupBackgroundTransparency = 0, -- BackgroundTransparency
    PopupTitle = Color3.fromHex("#FFFFFF"), -- Text
    PopupContent = Color3.fromHex("#FFFFFF"), -- Text
    PopupIcon = Color3.fromHex("#a1a1aa"), -- Icon

})

local Window = WindUI:CreateWindow({
    Title = "MidNight Hub",
    Icon = "moon", 
    Author = "By MiweR :D",
    Folder = "MidNightHub",
    Size = UDim2.fromOffset(600, 400),
    MinSize = Vector2.new(580, 380),
    MaxSize = Vector2.new(900, 600),
    Theme = "MiweR",
    Resizable = true,
    SideBarWidth = 180,
    Transparent = false,
    BackgroundImageTransparency = 0.5,
    HideSearchBar = true,
    ScrollBarEnabled = true,

    Background = "rbxassetid://16733302343"
})Window:SetToggleKey(Enum.KeyCode.Q)

local InformationTab = Window:Tab({
    Title = "Information",
    Icon = "book-text", 
    Locked = false,
})

local Paragraph = InformationTab:Paragraph({
    Title = "Welcome!",
    Desc = "This script its litteraly 1% produzed, just for test.. so you'll not use this script lol im tuff! #Miwer on discord thanks! Shadow me engole",
    Color = "Grey",
    Image = "",
    ImageSize = 30,
    Thumbnail = "",
    ThumbnailSize = 80,
    Locked = false,
})

local MainTab = Window:Tab({
    Title = "Main",
    Icon = "skull",
    Locked = false,
})

local Button = MainTab:Button({
    Title = "God Mode",
    Desc = "you're immortal, simple!",
    Locked = false,
    Callback = function()
        game:GetService("ReplicatedStorage").RemoteEvents.DamagePlayer:FireServer(-math.huge)
    
        WindUI:Notify({
            Title = "God Mode activated!",
            Content = "By now you're immortal.",
            Duration = 3, -- 3 seconds
            Icon = "skull",
        })
    end
})

InformationTab:Select()
